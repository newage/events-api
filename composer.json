{
  "name": "newage/events-api",
  "type": "library",
  "description": "Services for working with events",
  "license": "MIT",
  "keywords": [
    "events",
    "api"
  ],
  "homepage": "https://github.com/newage/events-api",
  "authors": [
    {
      "name": "Vadim Leontiev",
      "email": "vadim.leontiev@gmail.com",
      "homepage": "https://github.com/newage",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "roave/security-advisories": "dev-master",
    "zendframework/zend-expressive": "^1.0",
    "zendframework/zend-expressive-helpers": "^2.0",
    "zendframework/zend-stdlib": "^2.7 || ^3.0",
    "zendframework/zend-expressive-zendrouter": "^1.0",
    "zendframework/zend-servicemanager": "^2.7.3 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "squizlabs/php_codesniffer": "2.5.*",
    "phpmd/phpmd": "2.4.*",
    "phpspec/phpspec": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Events\\": "src/",
      "App\\": "src/App/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "scripts": {
    "test": "php vendor/bin/phpunit",
    "cs": "php vendor/bin/phpcs src --standard=PSR2 --colors",
    "cs-fix": "php vendor/bin/phpcbf src --standard=PSR2 --colors"
  }
}